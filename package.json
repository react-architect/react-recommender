{
  "name": "react-recommender",
  "version": "0.0.1-alpha.1",
  "description": "A recommender system for React",
  "author": "react-architect",
  "license": "MIT",
  "repository": "react-architect/react-recommender",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "jest": {
    "testMatch": [
      "**/*.steps.js"
    ]
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
