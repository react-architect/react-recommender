  "license": "MIT",
  "repository": "react-architect/react-recommender",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "doc": "rm -rf docs && npx styleguidist build",
    "deploy-doc": "npm run doc && gh-pages -d docs",
    "test": "cross-env CI=1 node scripts/test.js --env=jsdom",
    "test:watch": "node scripts/test.js --env=jsdom",
    "build": "tsc",
    "start": "npx styleguidist server"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "css-loader": "3.4.1",
    "gh-pages": "^1.2.0",
    "jest-cucumber": "^2.0.11",
    "react": "^16.4.1",
    "react-docgen": "^5.1.0",
    "react-docgen-typescript": "1.16.2",
    "react-dom": "^16.4.1",
    "react-styleguidist": "10.5.0",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.2",
    "ts-loader": "6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-dev-server": "3.10.1",
    "autoprefixer": "7.1.6",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "22.4.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
