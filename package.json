  "devDependencies": {
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.3.13",
    "@types/react-dom": "^16.0.5",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.1.4",
    "css-loader": "3.4.1",
    "gh-pages": "^1.2.0",
    "jest-cucumber": "^2.0.11",
    "react": "^16.4.1",
    "react-docgen": "^5.1.0",
    "react-docgen-typescript": "1.16.2",
    "react-dom": "^16.4.1",
    "react-scripts-ts": "^2.16.0",
    "react-styleguidist": "10.5.0",
    "source-map-loader": "0.2.4",
    "style-loader": "1.1.2",
    "ts-loader": "6.2.1",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-dev-server": "3.10.1"
  },
  "files": [
    "dist"
